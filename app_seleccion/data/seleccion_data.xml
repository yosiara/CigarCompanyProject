<?xml version="1.0"?>
<odoo>
   <data noupdate="1">



       <record id="seqp_app_seleccion" model="ir.sequence">
          <field name="name">codigo candidato</field>
          <field name="code">seq.seq</field>
          <field name="suffix">/%(year)s</field>
          <field name="use_date_range">True</field>
          <field name="padding">3</field>
       </record>

       <record id="seqc_solicitud_app_seleccion" model="ir.sequence">
          <field name="name">secuencia solicitud</field>
          <field name="code">seq.solicitud</field>
          <field name="use_date_range">True</field>
          <field name="padding">3</field>
       </record>

   </data>

</odoo>