<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="contract_internal_number_sequence" model="ir.sequence">
            <field name="name">Internal number contract</field>
            <field name="code">contract.internal.number</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="partner_archive_nro_sequence" model="ir.sequence">
            <field name="name">Partner archive number contract</field>
            <field name="code">partner.archive.number</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_contract_sequence" model="ir.sequence">
            <field name="name">Contract sequence</field>
            <field name="code">increment_seq_contract</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>